#include <stdio.h>
int findTrace(int mat[10][10], int n)
{
    int i, trace = 0;
    for(i = 0; i < n; i++)
    {
        trace = trace + mat[i][i];
    }
    return trace;
}

int main()
{
    int n, i, j, trace;
    int mat[10][10];
    printf("Enter order of square matrix: ");
    scanf("%d", &n);
    printf("Enter matrix elements:\n");
    for(i = 0; i < n; i++)
    {
        for(j = 0; j < n; j++)
        {
            scanf("%d", &mat[i][j]);
        }
    }
    trace = findTrace(mat, n);
    printf("Trace of matrix = %d", trace);
    return 0;
}
